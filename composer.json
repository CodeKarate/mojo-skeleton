{
    "name": "codekarate/mojo-skeleton",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "codekarate/mojo-core": "dev-dev"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/CodeKarate/mojo-core.git"
        }
    ],
    "authors": [
        {
            "name": "Shipu Ahamed",
            "email": "shipuahamed01@gmail.com"
        },
        {
            "name": "Salauddin Rana",
            "email": "rana7cse@gmail.com"
        }
    ],
    "extra": {
        "installer-paths": {
            "./core/": ["codekarate/mojo-core"]
        }
    },
    "config": {
      "preferred-install": "dist",
      "sort-packages": true,
      "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
